@model IEnumerable<PhotoListingServiceModel>

@foreach (var photo in Model)
{
    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading">
                <a asp-controller="Photos" asp-action="Details" asp-route-id="@photo.Id">
                    <img src="@photo.ImageUrl" alt="@photo.ImageUrl" width="100" height="100"/>
                </a>
            </div>
            <div class="panel-body">
                @photo.Likes | @photo.Comments - @photo.PublishDate.ToShortDateString()
            </div>
        </div>
    </div>

}