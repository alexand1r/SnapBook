@model IEnumerable<AdListingServiceModel>

@foreach (var ad in Model)
{
    <div class="panel-show col-xs-6 col-sm-4">
        <div class="panel panel-default">
            <a asp-controller="Users" asp-action="Ad" asp-route-id="@ad.Id">
                <div class="panel-heading">
                    <img src="@ad.AdProfilePicUrl" class="img-responsive img-listing" alt="@ad.AdProfilePicUrl" />
                </div>
                <div class="panel-body text-center">
                    <h5 class="wrap"><em>@ad.Name</em></h5>
                </div>
            </a>
        </div>
    </div>
}